{
    "name": "@aklinker1/github-contributors",
    "version": "0.0.4",
    "description": "A utility to get a list of contributors for a repo",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "index.d.ts"
    ],
    "scripts": {
        "build": "tsup index.js --format cjs,esm,iife --sourcemap --global-name githubContributors",
        "prepublish": "pnpm build"
    },
    "dependencies": {
        "ofetch": "^1.0.1"
    },
    "devDependencies": {
        "tsup": "^6.7.0",
        "typescript": "^5.0.2"
    }
}
